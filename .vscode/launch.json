    "version": "0.2.0",
    "configurations": [

        {
            "name": "Run Builder File-DB-DB+File",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Source/Ethanol.Cli/bin/Debug/net7.0/ethanol.exe",
            "args": [
                "run-builder",
                "-c",
                "context-builder.config.json"
            ],
            "cwd": "${workspaceFolder/TestData/ContextBuilder",
            "console": "externalTerminal",
            "env": {
                "POSTGRES_IP": "localhost",
                "POSTGRES_PORT": "1605",
                "POSTGRES_DATABASE": "ethanol",
                "POSTGRES_USER": "postgres",
                "POSTGRES_PASSWORD": "postgres"
            }
        },
        {
            "name": "Run Builder TCP-DB-File",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Source/Ethanol.Cli/bin/Debug/net7.0/ethanol.exe",
            "args": [
                "run-builder",
                "-c",
                "context-builder.tcp-in.config.json",
                "-p",
                "true"
            ],
            "cwd": "${workspaceFolder}/TestData/ContextBuilder",
            "console": "externalTerminal",
            "env": {
                "POSTGRES_IP": "localhost",
                "POSTGRES_PORT": "1605",
                "POSTGRES_DATABASE": "ethanol",
                "POSTGRES_USER": "postgres",
                "POSTGRES_PASSWORD": "postgres"
            }
        },
        {
            "name": "Start Service",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Source/Ethanol.Cli/bin/Debug/net7.0/ethanol.exe",
            "args": [
                "start-service",
                "-c",
                "context-provider.config.json"
            ],
            "cwd": "${workspaceFolder}/TestData/ContextProvider",
            "console": "externalTerminal",
            "env": {
                "POSTGRES_IP": "localhost",
                "POSTGRES_PORT": "1605",
                "POSTGRES_DATABASE": "ethanol",
                "POSTGRES_USER": "postgres",
                "POSTGRES_PASSWORD": "postgres"
            }
        },
        {
            "name": "Build Malware Models",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Source/Ethanol.Cli/bin/Debug/net7.0/ethanol.exe",
            "args": [
                "build-malware-profiles",
                "-r",
                "Train",
                "-o",
                "Models/lightblue.mal"
            ],
            "cwd": "${workspaceFolder}/TestData/MalwareSonar",
            "console": "externalTerminal"
        },
        {
            "name": "Test Malware Bening",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Source/Ethanol.Cli/bin/Debug/net7.0/ethanol.exe",
            "args": [
                "test-malware",
                "-p",
                "Models/lightblue.mal",
                "-i",
                "ctx-bening.json",
                "-o",
                "malware-test-bening.json",
                "-t",
                "0.5"
            ],
            "cwd": "${workspaceFolder}/TestData/MalwareSonar",
            //"console": "externalTerminal"
        },
        {
            "name": "Test Malware Infected",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Source/Ethanol.Cli/bin/Debug/net7.0/ethanol.exe",
            "args": [
                "test-malware",
                "-p",
                "Models/lightblue.mal",
                "-i",
                "ctx-infected.json",
                "-o",
                "malware-test-infected.json",
                "-t",
                "0.5"
            ],
            "cwd": "${workspaceFolder}/TestData/MalwareSonar",
            //"console": "externalTerminal"
        },
        {
            "name": "Infect Context",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Source/Ethanol.Cli/bin/Debug/net7.0/ethanol.exe",
            "args": [
                "infect-context",
                "-i",
                "ctx-bening.json",
                "-r",
                "Test",
                "-o",
                "ctx-infected.json",
            ],
            "cwd": "${workspaceFolder}/TestData/MalwareSonar",
            //"console": "externalTerminal"
        }  
    ]
}