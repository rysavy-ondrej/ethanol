    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Builder File-DB-DB+File",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Source/Ethanol.Cli/bin/Debug/net7.0/ethanol.exe",
            "args": [
                "run-builder",
                "-c",
                "context-builder.config.json"
            ],
            "cwd": "${workspaceFolder/TestData/ContextBuilder",
            "console": "externalTerminal",
            "env": {
                "POSTGRES_IP": "localhost",
                "POSTGRES_PORT": "1605",
                "POSTGRES_DATABASE": "ethanol",
                "POSTGRES_USER": "postgres",
                "POSTGRES_PASSWORD": "postgres"
            }
        },
        {
            "name": "Run Builder TCP-DB-File",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Source/Ethanol.Cli/bin/Debug/net7.0/ethanol.exe",
            "args": [
                "run-builder",
                "-c",
                "context-builder.tcp-in.config.json",
                "-p",
                "true"
            ],
            "cwd": "${workspaceFolder}/TestData/ContextBuilder",
            "console": "externalTerminal",
            "env": {
                "POSTGRES_IP": "localhost",
                "POSTGRES_PORT": "1605",
                "POSTGRES_DATABASE": "ethanol",
                "POSTGRES_USER": "postgres",
                "POSTGRES_PASSWORD": "postgres"
            }
        },
        {
            "name": "Start Service",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Source/Ethanol.Cli/bin/Debug/net7.0/ethanol.exe",
            "args": [
                "start-service",
                "-c",
                "context-provider.config.json"
            ],
            "cwd": "${workspaceFolder}/TestData/ContextProvider",
            "console": "externalTerminal",
            "env": {
                "POSTGRES_IP": "localhost",
                "POSTGRES_PORT": "1605",
                "POSTGRES_DATABASE": "ethanol",
                "POSTGRES_USER": "postgres",
                "POSTGRES_PASSWORD": "postgres"
            }
        }
    ]
}