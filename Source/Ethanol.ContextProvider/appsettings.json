{
  "AllowedHosts": "*",

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ApplicationUrl": "http://0.0.0.0:5190",
 
  "EthanolConfiguration": {
    "PostgresHost": "${POSTGRES_IP}",
    "PostgresPort": "${POSTGRES_PORT}",
    "PostgresUser": "${POSTGRES_USER}",
    "PostgresPassword": "${POSTGRES_PASSWORD}",
    "PostgresDatabase": "${POSTGRES_DATABASE}",
    "TagsTable": "enrichment_data",
    "HostContextTable": "host_context"
  }
}